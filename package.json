{
  "name": "dessert-store",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bunx tsx ./src/lib/server/db/seed.ts",
    "stripe:listen": "stripe listen --forward-to http://localhost:5173/api/stripe/webhook",
    "i18n:machine-translate": "bunx @inlang/cli machine translate --project project.inlang"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@lucide/svelte": "^0.507.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-netlify": "^5.0.2",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/slug": "^5.0.9",
    "autoprefixer": "^10.4.20",
    "bits-ui": "^1.4.6",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.30.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "formsnap": "^2.0.1",
    "globals": "^16.0.0",
    "mode-watcher": "^0.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-sonner": "^0.3.28",
    "sveltekit-superforms": "^2.24.1",
    "tailwind-merge": "^3.2.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.2.5",
    "zod": "^3.24.2"
  },
  "dependencies": {
    "@fontsource-variable/red-hat-text": "^5.2.5",
    "@inlang/paraglide-js": "^2.0.0",
    "@neondatabase/serverless": "^0.10.4",
    "@paralleldrive/cuid2": "^2.2.2",
    "@tanstack/svelte-query": "^5.74.4",
    "@tanstack/svelte-query-devtools": "^5.74.6",
    "@unpic/svelte": "^1.0.0",
    "better-auth": "^1.2.7",
    "currency.js": "^2.0.4",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.40.0",
    "resend": "^4.2.0",
    "slug": "^10.0.0",
    "stripe": "^18.1.0"
  }
}
